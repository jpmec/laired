<h2>Edit tilemap</h2>

<%= render "form.html", changeset: @changeset,
                        action: tilemap_path(@conn, :update, @tilemap) %>

<%= link "Back", to: tilemap_path(@conn, :index) %>


<div ng-controller='TilemapViewerController'>
  <div id="tilemap_viewer_div" class="col-md-12"></div>
  <pre>{{tilemap | json}}</pre>
</div>
